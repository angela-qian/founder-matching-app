// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String
  year      String   // freshman, soph, junior, senior
  major     String
  bio       String   // paragraph of how users describe themselves
  projectIdea String
  skills    String[]
  createdAt DateTime @default(now())
}
